footer{
    position: relative;
    z-index: 20;
    background-color: $color-theme;
    padding: 40px 0;
    .row{
        display: flex;
        margin: 0 -15px;
        flex-wrap: wrap;
    }
    .col-3{
        width: calc(3 / 12 * 100%);
        padding: 0 15px;
        @media screen and (max-width:  768px) {
            width: 50%;
        }
        @media screen and (max-width:  576px) {
            width: 100%;
            margin-bottom: 15px;
        }
    }
    .col-2{
        width: calc(2 / 12 * 100%);
        padding: 0 15px;
        @media screen and (max-width:  768px) {
            width: 50%;
        }
        @media screen and (max-width:  576px) {
            width: 100%;
            margin-bottom: 15px;
        }
    }
    .col-4{
        width: calc(4 / 12 * 100%);
        padding: 0 15px;
        @media screen and (max-width:  768px) {
            width: 50%;
        }
        @media screen and (max-width:  576px) {
            width: 100%;
            margin-bottom: 15px;
        }
    }
    .col-6{
        width: calc(6 / 12 * 100%);
        padding: 0 15px;
    }
    .footer-main{
        padding: 30px 0;
        position: relative;
    }
    .cap-hansogo{
        position: absolute;
        right: -180px;
        top: 200px;
        transform: rotate(-90deg);
        font-size: 105px;
        @media screen and (max-width: 1500px) {
            right: -120px;
            font-size: 75px;
        }
        @media screen and (max-width: 1300px) {
            right: -120px;
            top: 260px;
        }
        @media screen and (max-width: 1024px) {
            opacity: .3;
        }
        span{
            font-weight: bold;
            -webkit-text-fill-color: transparent;
            -webkit-text-stroke-width: 2px;
            -webkit-text-stroke-color: black;
        }
        color: black;
        font-weight: bold;
    }
    .box-foot{}
    .logo-ft{
        transform: translateX(-150px);
        width: 100%;
        @media screen and (max-width:1800px) {
            transform: translateX(0);
        }
        @media screen and (max-width: 768px) {
            width: 200px;
        }
        @media screen and (max-width: 576px) {
            width: 150px;
        }
    }
    .title-foot{
        position: relative;
        h4{
            color: #ffad33;
            font-size: 22px;
            font-weight: bold;
            text-transform: uppercase;
            @media screen and (max-width: 1024px) {
                font-size: 19px;
            }
        }
        &::before{
            content: '';
            position: absolute;
            bottom: -10px;
            left: 0;
            width: 100px;
            height: 3px;
            border: 2px solid #c0c0c0;
        }
    }
    .content-foot{
        font-size: 20px;
        padding-top: 10px;
        color: black;
        p{
            margin: 20px 0;
        }
        ul{
            margin-top: 20px;
        }
    }
}
