    overflow-y: hidden;
}

a {
    text-decoration: none;
    color: inherit;
}

li {
    list-style-type: none;
}
.mr-15{
    margin-right: 15px;
}
.all-setting{
    display: flex;
    @media screen and (max-width: 576px) {
        display: block;
    }
}
.btn-setting {
    width: 250px;
    height: 100%;
    padding: 10px;
    border-radius: 50px;
    align-items: center;
    display: flex;
    background-color: #252525;
    color: white;
    transition: .3s all ease-in-out;
    font-size: 14px;
    @media screen and (max-width: 576px) {
        margin-bottom: 10px;
    }
    &:hover {
        background-color: white;
        color: black !important;
    }
    .img-setting{
        width: 40px;
        margin-right: 10px;
    }
}
.btn {
    display: inline-block;
    vertical-align: bottom;
    appearance: none;
    -webkit-appearance: none;
    -moz-appearance: none;
    -ms-appearance: none;
    -o-appearance: none;
    border: none;
    height: 40px;
    width: 100%;
    line-height: 20px;
    background-color: transparent;
    -webkit-transition: all .3s ease;
    -moz-transition: all .3s ease;
    -o-transition: all .3s ease;
    -ms-transition: all .3s ease;
    transition: all .3s ease;
    padding: 10px 0px;
    text-align: center;
    text-transform: uppercase;
    font-weight: bold;
    position: relative;
    overflow: hidden;
    z-index: 0;
    outline: 0px;

    &.btn-search {
        width: 103px;
        height: 32px;
        background: #FECD06;
        color: #fff;
        border-radius: 99px;
        font-size: 14px;
        line-height: 13px;
        font-weight: normal;
        text-transform: none;

        &:hover {
            color: #FECD06;
            background: #fff;
        }
    }

    &.btn-main {
        width: 156px;
        height: 44px;
        border-radius: 99px;
        line-height: 25px;
    }

    &.btn-detail {
        width: 154px;
        height: 40px;
        background: $color-main;
        color: #fff;
        &:hover{
            background-color: white;
            border-radius: 30px;
            border: 1px solid $color-main;
            color: $color-main;
        }
    }
}

