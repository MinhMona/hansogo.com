                @media screen and (max-width: 576px) {
                    margin-bottom: 15px;
                }
                &:nth-child(1){
                    padding-left: 135px;
                    position: relative;
                    @media screen and (max-width: 1024px) {
                        padding-left: 0;
                    }
                    .box{
                        position: relative;
                        .text-box{
                            position: absolute;
                            bottom: 8px;
                            right: 10px;
                            width: 80px;
                            font-size: 22px;
                            text-transform: uppercase;
                            @media screen and (max-width: 1024px){
                                width: 125px;
                            }
                        }
                        .images-box{
                            position: absolute;
                            width: 200px;
                            left: -53%;
                            top: 20px;
                            img{
                                width: 100%;
                            }
                            @media screen and (max-width: 1024px) {
                                position: unset;
                            }
                        }
                    }
                }
                &:nth-child(2){
                    .box{
                        padding: 20px;
                        padding-right: 60px;
                        position: relative;
                        .text-box{
                            width: auto;
                            font-size: 22px;
                            text-transform: uppercase;
                            position: absolute;
                            bottom: 10px;
                            right: 20px;
                            span{
                                color: #c60f0f;
                                font-weight: bold;
                            }
                        }
                    }
                }
                &:nth-child(3){
                    .box{
                        position: relative;
                        .text-box{
                            width: auto;
                            padding: 15px;
                            font-size: 22px;
                            text-transform: uppercase;
                            span{
                                color: #c60f0f;
                                font-weight: bold;
                            }
                        }
                        .images-box{
                            position: absolute;
                            bottom: 10px;
                            right: 0;
                            @media screen and (max-width: 1024px) {
                                position: unset;
                            }
                        }
                    }
                }
            }
            .box{
                width: 250px;
                height: 350px;
                background-color: #dfdbd9;
                font-family: $font-babes;
                .text-box{
                    font-size: 32px !important;
                }
            }
        }
    }
}
