    margin-right: 15px;
}

.text-right {
    text-align: right;
}

.text-center {
    text-align: center;
}

.fcontrol {
    appearance: none;
    -webkit-appearance: none;
    -moz-appearance: none;
    -ms-appearance: none;
    -o-appearance: none;
    width: 100%;
    height: 100%;
    font-family: 'Roboto';
    font-size: 14px;
    border: solid 1px #ECECEC;
    padding: 10px 15px;
    line-height: 20px;
    outline: 0;
    -webkit-transition: all .3s ease-in-out;
    -moz-transition: all .3s ease-in-out;
    -o-transition: all .3s ease-in-out;
    -ms-transition: all .3s ease-in-out;
    transition: all .3s ease-in-out;
}

.border-radius-form {
    border-radius: 99px 0 0 99px;
}

