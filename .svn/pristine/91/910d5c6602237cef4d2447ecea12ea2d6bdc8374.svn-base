.find-prd-wrap {
    display: flex;
    flex-wrap: wrap;
    align-items: center;
}

.find-prd-wrap .title {
    width: 100%;
    color: #000;
    display: flex;
    justify-content: center;
    @media screen and (max-width: 576px) {
        transform: translateY(1px);
    }
}
.find-prd-wrap .title .hd {
    position: relative;
    text-transform: uppercase;
    width: 230px;
    max-width: 100%;
    text-align: center;
    padding: 8px 0;
    font-weight: 500;
    flex-shrink: 0;
    display: inline-block;
    cursor: pointer;
    font-size: 20px;
    // background: #fff;
    background-color: white;
    color: $color-main;
    border: 1px solid $color-main;
    transition: .3s all ease-in-out;
    &:hover{
        color: #fff;
        background: $color-main;
        img{
            filter: brightness(20);
        }
    }
    @media screen and (max-width: 768px) {
        font-size: 14px !important;
    }
}

.find-prd-wrap .title .hd img {
    margin-bottom: -5px;
    margin-right: 5px;
}
.find-prd-wrap{
    .title{
        h3{
            &:nth-child(1){
                border-radius: 50px 0 0 0;
            }
            &:nth-child(2){
                border-radius: 0 50px 0 0;
            }
        }
    }
}

.find-prd-wrap .title .hd.current {
    color: #fff;
    background: $color-main;
    .hd{
        &:last-child{
            box-shadow: 0 !important;
        }
    }
}

.find-prd-wrap .title .hd.current img {
    filter: brightness(20);
}

.search-form-wrap {
    width: 100%;
    padding: 20px 30px;
    background: $color-main;
    @media screen and (max-width: 576px) {
        height: 190px;
    }
}

.search-form-wrap .search-form {
    display: flex;
    position: relative;
    border: 1px solid #e1e1e1;
    background-color: #fff;
    @media screen and (max-width: 576px) {
        display: block;
    }
}

.search-form-wrap .search-form .dd {
    border: 0;
    border-radius: 0;
    outline: 0;
}

.search-form-wrap .search-form .dd .ddArrow {
    display: none;
}

.search-form-wrap .search-form .dd .ddChild::-webkit-scrollbar {
    width: 5px;
}

.search-form-wrap .search-form .dd .ddChild::-webkit-scrollbar-track {
    background: #f1f1f1;
}

.search-form-wrap .search-form .dd .ddChild::-webkit-scrollbar-thumb {
    background: #888;
    border-radius: 5px;
}

.search-form-wrap .search-form .dd .ddChild li {
    display: flex;
    align-items: center;
}

.search-form-wrap .search-form .dd .ddChild li img {
    max-width: 35px;
}

.search-form-wrap .search-form .dd .divider {
    display: none;
}

.search-form-wrap .search-form .dd .ddTitle {
    background: #fff;
    border-radius: 0;
}

.search-form-wrap .search-form .dd .ddTitle .ddTitleText {
    display: flex;
    align-items: center;
    position: relative;
}

.search-form-wrap .search-form .dd .ddTitle .ddTitleText:before {
    content: '';
    position: absolute;
    right: 0;
    top: 6px;
    height: 30px;
    width: 1px;
    background: #80808063;
    @media screen and (max-width: 576px) {
        display: none;
    }

}

.search-form-wrap .search-form .dd .ddTitle .ddTitleText img {
    display: block;
    height: 30px;
    width: 30px;
    -o-object-fit: contain;
    object-fit: contain;
}

.search-form-wrap .search-form .dd .ddlabel {
    font-size: 13px;
    font-weight: 500;
    color: black;
    display: inline-block;
    vertical-align: middle;
    padding: 8px;
    line-height: 1;
    max-width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
}

.search-form-wrap .search-form .select-form {
    min-height: 42px;
    width: 145px;
    flex-shrink: 0;
    position: relative;
    @media screen and (max-width: 576px) {
        width: 100%;
    }
}

.search-form-wrap .search-form .select-form .icon {
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    right: 15px;
    pointer-events: none;
    color: $color-main;
}

.search-form-wrap .search-form .main-btn {
    position: absolute;
    right: 1px;
    top: 1px;
    z-index: 1;
    width: 126px;
    height: 40px;
    padding: 0;
    /* padding-left: 20px; */
    text-align: center;
    line-height: 40px;
    white-space: nowrap;
    background: $color-main;
    color: #fff;
    font-weight: bold;
    text-transform: uppercase;
    font-size: 14px;
    background-position: center center;
    background-size: cover;
    -webkit-transition: 0.3s ease;
    transition: 0.3s ease;
    &:hover{
        background-color: white;
        color: $color-main;
        border: 1px solid $color-main;
        border-radius: 30px;
        .search-form-wrap .input-form .f-input{
            border-radius: 0 30px 30px 0 !important;
        }
    }
    @media screen and (max-width: 576px) {
        top: 110px;
        background-color: white;
        color: $color-main;
    }
}

.search-form-wrap .input-form {
    width: 100%;
    position: relative;
    padding-left: 20px;
    @media screen and (max-width: 576px) {
        padding-left: 0;
    }
}

.search-form-wrap .input-form .f-input {
    border: solid 0px #ECECEC;
    height: 42px;
}


.search-form-wrap .search-form .select-form select {
    background-size: cover;
    border: solid 0px #ececec;
}

.find-prd-wrap .title .hd.right {
    margin-left: -3px;
}
