.table-service {
    .columns {
        box-shadow: 1px 5px 16px rgba(0, 0, 0, 0.2);
        padding: 20px;
        margin-top: 15px;
        background: #fff;
        .colum {
            width: calc(4 / 12 * 100%);
            height: auto;
            &:nth-child(1) {
                border-right: 1px solid rgba(0, 0, 0, 0.1);
                border-bottom: 1px solid rgba(0, 0, 0, 0.1);
            }

            &:nth-child(2) {
                border-right: 1px solid rgba(0, 0, 0, 0.1);
                border-bottom: 1px solid rgba(0, 0, 0, 0.1);
            }

            &:nth-child(3) {
                border-bottom: 1px solid rgba(0, 0, 0, 0.1);
            }

            &:nth-child(4) {
                border-right: 1px solid rgba(0, 0, 0, 0.1);
            }

            &:nth-child(5) {
                border-right: 1px solid rgba(0, 0, 0, 0.1);
            }

            &:hover .content:before {
                opacity: 1;
            }

            &:hover .content .title-intro {
                color: $color-main;
            }

            &:hover .content .btn-detail {
                opacity: 1;
            }

            &:hover .content .icon:before {
                background: $color-main;
            }

            .content {
                padding: 15px 15px;
                position: relative;

                &:before {
                    content: '';
                    width: 0;
                    height: 0;
                    border-style: solid;
                    bottom: -15px;
                    position: absolute;
                    right: -15px;
                    border-width: 0 0 30px 30px;
                    border-color: transparent transparent $color-main transparent;
                    opacity: 0;
                    transition: 0.3s ease;
                }

                .icon {
                    padding-bottom: 17.5px;
                    position: relative;
                    text-align: center;
                    margin-bottom: 17.5px;

                    &:before {
                        content: '';
                        position: absolute;
                        bottom: 0;
                        left: 50%;
                        transform: translateX(-50%);
                        width: 100px;
                        height: 1.5px;
                        background: rgba(0, 0, 0, 0.2);
                        transition: 0.3s ease;
                    }
                }

                .title-intro {
                    color: #000;
                    font-size: 18px;
                    font-weight: 500;
                    text-transform: uppercase;
                    text-align: center;
                    margin-bottom: 15px;
                    transition: 0.3s ease;
                }

                .desc-intro {
                    text-align: center;
                    margin-bottom: 20px;
                }

                .btn-detail {
                    margin: 0 auto;
                    opacity: 0;
                    box-shadow: 1px 5px 16px rgba(0, 0, 0, 0.25);
                    &:hover{
                        background-color: white;
                        color: $color-main;
                        border-radius: 30px;
                        border: 1px solid $color-main;
                    }
                }
            }
        }
    }
}

.arrow-up {
    position: absolute;
    left: 0;
    top: -25px;

    &:before {
        content: '';
        position: absolute;
        width: 1px;
        height: 140px;
        top: -130px;
        right: 9px;
        background: #fff;
    }
}

.color-fff {
    color: #fff !important;
}

.arrow-down {
    position: absolute;
    left: 0;
    top: 55px;

    &:before {
        content: '';
        position: absolute;
        width: 1px;
        height: 355px;
        top: 6px;
        right: 9px;
        background: #fff;
    }
}

.table-resgister {
    position: relative;
    z-index: 1;
    .c-tab {
        display: flex;
        justify-content: flex-end;
        flex-direction: row-reverse;
        flex-wrap: wrap;
        .c-tab__nav {
            width: calc(4 / 12 * 100%);
            position: relative;
            ul {
                li {
                    color: #fff;
                    text-transform: uppercase;
                    font-size: 18px;
                    font-weight: 500;
                    display: flex;
                    justify-content: flex-end;
                    cursor: pointer;
                    padding: 10px 0;
                    display: flex;
                    i {
                        font-size: 30px;
                        display: none;
                    }

                    &.active {
                        color: $color-main;
                    }

                    &.active i {
                        display: block;
                        transform: translateX(-10px);
                    }
                }
            }
        }

        .c-tab__content {
            width: calc(8 / 12 * 100%);
            display: none;
            position: relative;

            .box-img {
                margin-top: -20px;
            }

            .content {
                padding: 40px;
                position: relative;
                background: #fff;
                max-width: 410px;
                position: absolute;
                top: 100px;
                left: 35px;

                &:before {
                    content: '';
                    width: 0;
                    height: 0;
                    border-style: solid;
                    bottom: 0;
                    position: absolute;
                    right: 0;
                    border-width: 0 0 30px 30px;
                    border-color: transparent transparent $color-main transparent;
                    transition: 0.3s ease;
                }

                .title-intro {
                    color: $color-main;
                    font-size: 18px;
                    font-weight: 500;
                    text-transform: uppercase;
                    margin-bottom: 15px;
                    transition: 0.3s ease;
                }

                .desc-intro {
                    margin-bottom: 20px;
                }

                .btn-detail {
                    margin: 0 auto;
                    box-shadow: 1px 5px 16px rgba(0, 0, 0, 0.25);
                }
            }

            &.active {
                display: block;
            }
        }
    }
}


.copy-right {
    text-align: center;
    background: $color-main;
